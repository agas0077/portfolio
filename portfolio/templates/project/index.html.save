{% extends "base.html" %}
{% block content %}
	{{ user.projects }}
    <div class="primary col-lg-8 col-12">
        {% if user.about %}{% include "includes/project/about.html" %}{% endif %}
        % if user.projects %}{% include "includes/project/project_list.html" %}{% endif %}
        {% comment %} {% include "includes/project/other_project_list.html" %} {% endcomment %}
        {% if user.jobs.all|length > 0 %}{% include "includes/project/work_experience.html" %}{% endif %}
        {% comment %} {% include "includes/project/github.html" %} {% endcomment %}
        {% if user.city and user.country and user.email %}{% endif %}{% include "includes/project/contacts.html" %} 
    </div>
    <div class="primary col-lg-4 col-12">
        {% if user.skills.all|length > 0 %}{% include "includes/project/skills.html" %}{% endif %}
        {% if user.recomendations.all|length > 0 %}{% include "includes/project/recomendations.html" %}{% endif %}
        {% if user.languages.all|length > 0 %}{% include "includes/project/languages.html" %}{% endif %}
        {% if user.educations.all|length > 0 %}{% include "includes/project/education.html" %}{% endif %}
    </div>
{% endblock content %}
